				"-I",
				"/usr/include/x86_64-linux-gnu/qt5/QtWebSockets/",
				"-I",
				"/usr/include/x86_64-linux-gnu/qt5/QtSerialPort/",
				"-I",
				"/usr/include/x86_64-linux-gnu/qt5/QtNetwork/",
				"-I",
